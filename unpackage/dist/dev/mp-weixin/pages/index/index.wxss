.content {
  width: 100vw;
  height: 100vh;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  justify-items: center;
  background-color: #F2F2F2;
}
.header {
  width: 694rpx;
  height: 261rpx;
  border-radius: 20rpx;
  margin-top: 18rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  overflow: hidden;
}
.header > image {
  width: 100%;
  height: 100%;
  object-fit: contain;
}
.ullist {
  width: 694rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  margin-top: 17rpx;
  border-radius: 20rpx;
  overflow: hidden;
  background-color: #FFFFFF;
}
.ullist .items {
  width: 50%;
  height: 172rpx;
  background-color: #FFFFFF;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-bottom: 2rpx solid #EFEFEF;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  padding-left: 18rpx;
  box-sizing: border-box;
}
.ullist > .items:nth-child(2n+1) {
  border-right: 2rpx solid #EFEFEF;
}
.iconBox {
  width: 62rpx;
  height: 62rpx;
  border-radius: 50%;
  background-color: #DEECFF;
}
.titleBox {
  padding-left: 12rpx;
  box-sizing: border-box;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  position: relative;
}
.itemTitle, .itemDle {
  color: #333;
  font-weight: bold;
  font-size: 28rpx;
}
.itemDle {
  color: #A2A2A2;
  font-weight: 400;
  margin-top: 18rpx;
}
.gzhBox {
  width: 150rpx;
  height: 60rpx;
  position: fixed;
  right: 0;
  bottom: 100rpx;
  overflow: hidden;
}
.hzBox {
  width: 200rpx;
  height: 60rpx;
  border-radius: 40rpx;
  background-color: #5393FD;
  background-image: -webkit-linear-gradient(left, #5393FD, #FA7240);
  background-image: linear-gradient(to right, #5393FD, #FA7240);
  color: #fff;
  font-size: 26rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  line-height: 60rpx;
  padding-left: 26rpx;
  box-sizing: border-box;
}
.iconBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  font-size: 42rpx;
  color: #5C96FC;
}
.linkbtn {
  position: absolute;
  z-index: 9;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  opacity: .01;
}

